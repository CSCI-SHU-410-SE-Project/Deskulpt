{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "manager-capabilities",
  "description": "Capabilities of the manager in addition to the common capabilities",
  "local": true,
  "windows": ["manager"],
  "permissions": [
    "allow-exit-app",
    "allow-init-settings",
    "allow-open-widget-base",
    "allow-refresh-widget-collection",
    "allow-toggle-click-through",
    "global-shortcut:allow-register",
    "global-shortcut:allow-unregister"
  ]
}
