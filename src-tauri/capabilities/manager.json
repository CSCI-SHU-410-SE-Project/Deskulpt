{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "manager-capabilities",
  "description": "Capabilities of the manager in addition to the common capabilities",
  "local": true,
  "windows": ["manager"],
  "permissions": [
    "allow-bundle-external-dependencies",
    "allow-exit-app",
    "allow-init-settings",
    "allow-open-widget-resource",
    "allow-refresh-widget-collection",
    "allow-register-toggle-shortcut",
    "shell:allow-open"
  ]
}
